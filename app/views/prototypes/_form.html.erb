<%= form_with local: true do |f|%>
  <div class="field">
    <%= f.label :filed, "プロトタイプの名称" %><br />
    <%= f.text_field :filed %>
  </div>

  <div class="field">
    <%= f.label :cach_copy, "キャッチコピー" %><br />
    <%= f.text_area :cach_copy, class: :form__text %>
  </div>

  <div class="field">
    <%= f.label :consept, "コンセプト" %><br />
    <%= f.text_area :consept, class: :form__text %>
  </div>

  <div class="field">
    <%= f.label :image, "プロトタイプの画像" %><br />
    <%= f.file_field :image %>
  </div>

  <div class="actions">
    <%= f.submit "保存する", class: :form__btn  %>
  </div>
<% end %>